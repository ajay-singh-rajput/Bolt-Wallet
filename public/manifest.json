{
    "manifest_version": 3,
    "name": "My React Extension",
    "version": "1.0",
    "description": "A Chrome extension built with React.",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "../index.html",
      "default_icon": "icon-wallet.webp"
    },
    "permissions": ["storage"]
  }
  